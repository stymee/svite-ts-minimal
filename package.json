{
    "name": "svite-example-minimal-typescript",
    "version": "0.0.0",
    "scripts": {
        "validate": "svelte-check && tsc --noEmit",
        "dev": "svite -ts --port 3400",
        "build": "svite build -ts --base /svite/",
        "format": "prettier --write src/**/*.{css,html,js,ts,svelte}"
    },
    "dependencies": {
        "dayjs": "^1.10.4",
        "query-string": "^6.13.8",
        "svelte-routing": "^1.5.0"
    },
    "devDependencies": {
        "@tsconfig/svelte": "^1.0.10",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.1.1",
        "svelte": "^3.29.7",
        "svelte-check": "^1.1.15",
        "svelte-hmr": "^0.11.6",
        "svelte-preprocess": "^4.6.1",
        "svite": "latest",
        "tslib": "^2.0.3",
        "typescript": "^4.1.2"
    }
}
